{
  "name": "danvaly/prime-datasource",
  "description": "Ng Prime Datasource",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Valentin Ghica",
      "email": "git@danvaly.ro"
    }
  ],
  "require": {
    "php": "^7.4|^8.0",
    "illuminate/database": "^8.37|^9.0|^10.0"
  },
  "require-dev": {
    "orchestra/testbench": "^6|^7|^8.0",
    "mockery/mockery": "^1.3.3",
    "phpunit/phpunit": ">=8.5.23|^9",
    "laravel/scout": "^9.4"
  },
  "autoload": {
    "psr-4": {
      "Danvaly\\PrimeDatasource\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Danvaly\\PrimeDatasource\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Danvaly\\PrimeDatasource\\DatasourceProvider"
      ]
    }
  }
}
